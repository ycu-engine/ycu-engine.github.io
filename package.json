{
  "name": "ore-ore-gatsbyjs-template",
  "description": "Gatsby OreOre TypeScript starter",
  "version": "1.0.0",
  "author": "YutaUra<yuuta3594@outlook.jp>",
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "yarn format:ts && yarn format:md",
    "format:ts": "prettier --parser typescript --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.ts*\"",
    "format:md": "prettier --parser mdx --write \"src/**/*.md*\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "husky install && typesync || :",
    "type-check": "tsc",
    "format-check": "yarn format-check:ts && yarn format-check:md",
    "format-check:ts": "prettier --parser typescript --trailing-comma es5 --no-semi --single-quote -c \"src/**/*.ts*\"",
    "format-check:md": "prettier --parser mdx -c \"src/**/*.md*\"",
    "lint": "yarn lint:md && yarn lint:ts",
    "lint:md": "yarn eslint src --ext .md,.mdx",
    "lint:ts": "yarn eslint src --ext .ts,.tsx"
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.0.0-next.8",
    "@mdx-js/react": "^1.6.22",
    "autoprefixer": "^10.2.4",
    "date-fns": "^2.17.0",
    "date-fns-tz": "^1.1.1",
    "eslint-plugin-mdx": "^1.8.2",
    "gatsby": "^2.32.4",
    "gatsby-image": "^2.11.0",
    "gatsby-link": "^2.11.0",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-mdx": "^1.10.0",
    "gatsby-plugin-postcss": "^3.7.0",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-sharp": "^2.14.1",
    "gatsby-source-filesystem": "^2.11.0",
    "gatsby-transformer-sharp": "^2.12.0",
    "postcss": "^8.2.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "recoil": "^0.1.2",
    "tailwindcss": "^2.0.3"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.6",
    "@types/mdx-js__react": "^1.5.3",
    "@types/node": "^14.14.30",
    "@types/prettier": "^2.2.1",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "@types/react-helmet": "^6.1.0",
    "@types/string-similarity": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "@typescript-eslint/types": "^4.15.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-rulesdir": "^0.1.0",
    "husky": "^5.0.9",
    "pinst": "^2.1.4",
    "prettier": "^2.2.1",
    "string-similarity": "^4.0.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5",
    "typesync": "^0.8.0",
    "yaml-ast-parser": "^0.0.43"
  },
  "contributors": [
    "YutaUra<yuuta3594@outlook.jp>"
  ],
  "browserslist": [
    "defaults"
  ]
}
