{
  "name": "ore-ore-gatsbyjs-template",
  "description": "Gatsby OreOre TypeScript starter",
  "version": "1.0.0",
  "author": "YutaUra<yuuta3594@outlook.jp>",
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build --log-pages",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "husky install && typesync || :",
    "type-check": "tsc",
    "format-check": "run-p format-check:*",
    "format-check:ts": "prettier --parser typescript --trailing-comma es5 --no-semi --single-quote -c \"src/**/*.ts*\"",
    "format-check:md": "prettier --parser mdx -c \"src/**/*.md*\"",
    "format:md": "prettier --parser mdx -w \"src/**/*.md*\"",
    "lint": "run-p lint:*",
    "lint:md": "yarn eslint src --ext .md,.mdx",
    "lint:ts": "yarn eslint src --ext .ts,.tsx"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --parser typescript --write"
    ],
    "*.{md,mdx}": [
      "prettier --parser mdx --write"
    ],
    "*.{js,jsx,ts,tsx,md,mdx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.0.0-next.8",
    "@mdx-js/react": "^1.6.22",
    "autoprefixer": "^10.4.9",
    "date-fns": "^2.29.2",
    "date-fns-tz": "^1.3.7",
    "eslint-plugin-mdx": "^1.17.1",
    "gatsby": "^3.12.1",
    "gatsby-link": "^4.22.0",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-google-gtag": "^3.14.0",
    "gatsby-plugin-graphql-codegen": "^3.1.1",
    "gatsby-plugin-image": "^1.4.1",
    "gatsby-plugin-manifest": "^3.14.0",
    "gatsby-plugin-mdx": "^2.12.0",
    "gatsby-plugin-postcss": "^4.14.0",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-robots-txt": "^1.7.1",
    "gatsby-plugin-sharp": "^3.14.3",
    "gatsby-plugin-sitemap": "^4.10.0",
    "gatsby-source-filesystem": "^3.14.0",
    "gatsby-transformer-sharp": "^3.14.0",
    "lodash-es": "^4.17.21",
    "postcss": "^8.4.16",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "recoil": "^0.7.5",
    "tailwindcss": "^3.1.8"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.2",
    "@types/lodash-es": "^4.17.6",
    "@types/mdx-js__react": "^1.5.5",
    "@types/node": "^18.7.16",
    "@types/prettier": "^2.7.0",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.3",
    "@types/react-helmet": "^6.1.5",
    "@types/string-similarity": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@typescript-eslint/types": "^5.37.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-rulesdir": "^0.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.2",
    "npm-run-all": "^4.1.5",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "string-similarity": "^4.0.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "typesync": "^0.9.2",
    "yaml-ast-parser": "^0.0.43"
  },
  "contributors": [
    "YutaUra<yuuta3594@outlook.jp>"
  ],
  "browserslist": [
    "defaults"
  ]
}
